{
  "compilerOptions": {
    "target": "es5",
    "composite": true,
    "useDefineForClassFields": true,
    "lib": [
      "es2016",
      "dom"
    ],
    "allowJs": false,
    "skipLibCheck": true,
    "esModuleInterop": false,
    "allowSyntheticDefaultImports": true,
    "strict": false,
    "forceConsistentCasingInFileNames": false,
    "module": "commonjs",
    "moduleResolution": "Node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react",
    "typeRoots": [
      "node_modules/@types",
      "src/types"
    ]
  },
  "include": [
    "./src/**/*.ts",
    "src/router/index.tsx",
    "src/components/Router-Security/PrivateRouter.tsx",
    "src/@types/.d.ts",
    "src/components/Lazy/LazyComponent.tsx",
    "src/components/Message/Warning.tsx",
    "src/components/Message/Error.tsx",
    "src/hook/form/index.tsx",
    "src/components/Form/index.tsx",
    "src/components/Particles/index.tsx",
    "src/auth/getUser.tsx"
, "src/components/Button/index.tsx", "src/components/Divider/index.tsx", "src/components/Icon/index.tsx"  ],
  "references": [
    {
      "path": "./tsconfig.node.json"
    }
  ],
}